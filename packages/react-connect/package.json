{
  "name": "@findify/react-connect",
  "version": "0.1.9",
  "author": "Findify <yourfriends@findify.io>",
  "bugs": {
    "url": "https://github.com/findify/findify-js/issues"
  },
  "config": {
    "access": "public"
  },
  "dependencies": {
    "@findify/agent": "^1.2.4",
    "@findify/analytics": "^3.2.1",
    "react": "^16.4.0",
    "immutable": "^4.0.0-rc.9"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.41",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.41",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.41",
    "@babel/preset-env": "^7.0.0-beta.41",
    "@babel/preset-typescript": "^7.0.0-beta.41",
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "@types/react": "^16.0.40",
    "@types/webpack": "^4.1.0",
    "babel-loader": "^8.0.0-beta.0",
    "del-cli": "^1.1.0",
    "dotenv": "^5.0.1",
    "duplicate-package-checker-webpack-plugin": "^2.1.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "jest": "^22.4.2",
    "microtime": "^2.1.7",
    "npm-check-updates": "^2.14.1",
    "npm-run-all": "^4.1.2",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^22.4.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "typedoc": "^0.11.1",
    "typescript": "2.7.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-bundle-analyzer": "^2.11.1"
  },
  "homepage": "https://github.com/findify/findify-js/packages/agent",
  "keywords": [
    "findify"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/findify/findify-js.git"
  },
  "scripts": {
    "build": "run-p build:lib build:dist",
    "build:dist": "webpack",
    "build:dist:analyze": "npm run build:dist -- --env.analyze",
    "build:lib": "tsc --project tsconfig.lib.json",
    "build:types": "tsc --project tsconfig.lib.json  --declaration --declarationDir types",
    "build:test": "npm run build:lib -- --project tsconfig.test.json --module commonjs",
    "build:test:watch": "npm run build:test -- --watch",
    "clean": "del lib types",
    "docs": "typedoc --name 'Findify Agent' --hideGenerator --target es5 --theme default --out doc src",
    "lint": "tslint --project tsconfig.json --type-check --format codeFrame src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "test": "# jest",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch"
  },
  "types": "types/index.d.ts"
}
