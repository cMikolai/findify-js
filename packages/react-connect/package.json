    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-typescript": "^7.0.0-beta.38",
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "@types/react": "^16.0.35",
    "@types/webpack": "^3.8.3",
    "babel-loader": "^8.0.0-beta.0",
    "del-cli": "^1.1.0",
    "dotenv": "^4.0.0",
    "duplicate-package-checker-webpack-plugin": "^2.1.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "jest": "^22.1.4",
    "microtime": "^2.1.7",
    "npm-check-updates": "^2.14.0",
