{
  "name": "@findify/sdk",
  "version": "1.3.9",
  "description": "Findify search API client",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/findify/findify-js.git"
  },
  "keywords": [
    "findify"
  ],
  "config": {
    "access": "public"
  },
  "author": "Findify <yourfriends@findify.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/findify/findify-js/issues"
  },
  "homepage": "https://github.com/findify/findify-js",
  "scripts": {
    "clean": "del lib types",
    "build": "run-p build:lib build:dist",
    "build:dist": "TS_NODE_COMPILER_OPTIONS=\"{\\\"module\\\":\\\"commonjs\\\"}\" webpack",
    "build:dist:analyze": "npm run build:dist -- --env.analyze",
    "build:lib": "tsc --project tsconfig.lib.json --declaration --declarationDir types",
    "build:test": "npm run build:lib -- --module commonjs",
    "build:test:watch": "npm run build:test -- --watch",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch",
    "lint": "tslint --project tsconfig.json --type-check --format codeFrame src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "docs": "typedoc --name 'Findify SDK' --hideGenerator --target es5 --theme default --out doc src"
  },
  "dependencies": {
    "@types/qs": "^6.5.0",
    "@types/jsonp": "^0.2.0",
    "axios": "^0.16.2",
    "jsonp": "^0.2.1",
    "debug": "^3.1.0",
    "core-js": "^2.5.1",
    "qs": "^6.5.1"
  },
  "devDependencies": {
    "nock": "^9.0.22",
    "@types/nock": "^8.2.1",
    "@types/jest": "^21.1.2",
    "@types/node": "^8.0.34",
    "@types/webpack": "^3.0.13",
    "ts-node": "^3.3.0",
    "npm-run-all": "^4.1.1",
    "del-cli": "^1.1.0",
    "dotenv": "^4.0.0",
    "jest": "^21.1.0",
    "ts-jest": "^21.1.0",
    "npm-check-updates": "^2.12.1",
    "tslint": "^5.7.0",
    "tslint-config-airbnb": "^5.3.0",
    "tslint-config-prettier": "^1.5.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.5.3",
    "webpack": "^3.6.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "webpack-bundle-analyzer": "^2.9.0"
  }
}
