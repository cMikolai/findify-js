{
  "name": "@findify/sdk",
  "description": "Findify search api client",
  "version": "1.3.12",
  "author": "Findify <yourfriends@findify.io>",
  "dependencies": {
    "axios": "^0.16.2",
    "bluebird": "^3.5.0",
    "jsonp": "^0.2.1",
    "lodash": "^4.17.4",
    "qs": "^6.5.1"
  },
  "devDependencies": {
    "@types/axios": "^0.9.34",
    "@types/bluebird": "^3.5.0",
    "@types/expect": "^1.20.2",
    "@types/jsdom": "^2.0.29",
    "@types/lodash": "^4.14.76",
    "@types/mocha": "^2.2.43",
    "@types/qs": "^6.5.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.0.5",
    "expect": "^1.20.2",
    "faux-jax": "^5.0.6",
    "jsdom": "^9.12.0",
    "mocha": "^3.5.3",
    "npm-run-all": "^4.1.1",
    "rewire": "^2.5.2",
    "rimraf": "^2.6.2",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3",
    "webpack": "^1.14.0",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0"
  },
  "files": [
    "dist",
    "lib",
    "index.d.ts"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "run-p build:commonjs build:umd build:umd:min",
    "build:commonjs": "run-p ts:check cp:env",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.ts dist/findify-sdk.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.ts dist/findify-sdk.min.js",
    "check": "npm run test && npm run build && npm run lint && npm run clean",
    "clean": "rimraf lib dist",
    "ts:check": "tsc --project .",
    "cp:env": "cpx './src/env/*.js' ./lib/env",
    "lint": "tslint --project .",
    "postpublish": "npm run clean",
    "postversion": "git push && git push --tags && git checkout develop && git merge master && git push",
    "preversion": "npm run check && git checkout master && git merge develop",
    "test": "cross-env mocha --compilers ts:ts-node/register --recursive -R min  ./__tests__"
  },
  "tags": [
    "client",
    "search",
    "findify",
    "sdk"
  ]
}
